<% layout('layouts/boilerplate') %>
<div class="row">
    <div class="col-6 offset-3">
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading"><%= err.message %></h4>
            <% if (process.env.NODE_ENV !== "production") { %> 
            <p><%= err.stack %></p>
            <% } %> 
            <hr>
            <p class="mb-0">If you see this message, please send a screenshot to our team for assistance at <a
                href="mailto:yelpcamp@email.com">yelpcamp@email.com</a>.</p>
        </div>
    </div>
</div>